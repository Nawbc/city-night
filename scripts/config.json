{
	"port": 3000,
	"rootPath": "./",
	"publicExtUrl": "/",
	"constPaths": {
		"tsConfig": "./tsconfig.json",
		"packageJson": "./package.json",
		"nodeModules": "./node_modules",
		"appSrc": "./app",
		"appHtml": "./app/public/index.htm",
		"appPublic": "./app/public",
		"appEntryTsx": "./app/index.tsx",
		"appOutput": "./release/app",
		"libSrc": "./lib",
		"pluginsSrc": "./plugins"
	},
	"release": {
		"silent-concept.js": {
			"entry": "./lib/index.tsx",
			"output": "./release/dist",
			"library": "Silent",
			"libraryTarget": "umd"
		},
		"table.js": {
			"entry": "./lib/component/Table/index.tsx",
			"output": "./release/components/table",
			"library": "Table",
			"libraryTarget": "commonjs2"
		}
	},
	"interpolateHtml": {
		"themeColor": "#FFFFFF"
	}
}
