{
	"port": 3000,
	"paths": {
		"itemRootPath": ".",
		"sitePath": "muguet-site",
		"libPath": "src",
		"releaseEs": "release/es",
		"releaseLib": "release/lib",
		"releaseDist": "release/dist",
		"tsConfigPath": "tsconfig.json",
		"libOutputPath": "release/dist",
		"libEntryPath": "src/exports.tsx",
		"siteEntryPath": "muguet-site/site.tsx",
		"siteOutputPath": "release-muguet-site",
		"sitePublicPath": "muguet-site/public",
		"siteHtmlPath": "muguet-site/public/index.html",
		"appNodeModules": "node_modules"
	},
	"urls": {
		"gradients-src": [
			"https://webgradients.com/",
			"https://www.grabient.com/"
		]
	},
	"package": {
		"outputCssFileName": "muguet.css",
		"filename": "muguet.js",
		"library": "Muguet",
		"libraryTarget": "umd",
		"alias_src": "Muguet",
		"alias_site": "MuguetSite"
	}
}
