{
	"author": "Han Wang",
	"version": "0.0.1",
	"name": "silent-concept",
	"description": "",
	"main": "./release/lib/index.js",
	"module": "./release/es/index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=node scripts/build.js",
		"dist:production": "cross-env NODE_ENV=production node scripts/build.js",
		"dist:development": "cross-env NODE_ENV=development node scripts/build.js",
		"dist": "npm-run-all dist:*",
		"tsc": "tsc",
		"es": "node bin/release.es.lib.js  es",
		"es:tsc": "tsc ./release/es -p ./tsc.es.json",
		"es:dts": "tsc -d --emitDeclarationOnly -p ./tsc.es.json --declarationDir ./release/es",
		"lib": "node bin/release.es.lib.js  lib",
		"lib:babel": "cross-env NODE_ENV=production BABEL_ENV=production babel lib --out-dir release/lib --extensions .tsx --source-maps",
		"lib:tsc": "tsc -d --emitDeclarationOnly -p ./includeLib.json --declarationDir ./release/lib",
		"plugins": "node bin/release.plugins.js",
		"plugins:tsc": "tsc --outDir ./release/plugins  -p ./tsc.plugins.json",
		"plugins:dts": "tsc -d --emitDeclarationOnly -p ./tsc.plugins.json --declarationDir ./release/plugins",
		"scss:lib": "node-sass lib -o release/lib --importer scripts/utils/compass.js --source-map true --output-style compressed",
		"scss:es": "node-sass lib -o release/es --importer scripts/utils/compass.js --source-map true --output-style compressed",
		"scss:plugins": "node-sass lib -o release/plugins --importer scripts/utils/compass.js --source-map true --output-style compressed",
		"release:all": "npm run dist && npm run release:app && npm run lib && npm run es && npm run plugins",
		"release:app": "cd ./app && npm run build",
		"test": "npm run test:unit && npm run test:e2e",
		"test:unit": "jest -u",
		"test:e2e": "jest --config ./jest.e2e.js",
		"test:ci": "cross-env CI=true node scripts/utils/ciTest.js",
		"auto:lang": "node ./scripts/auto/langTypesMaker.js",
		"auto:gradients": "node ./scripts/auto/gradientsMaker.js",
		"auto:svg": "node ./scripts/auto/svgModuleMaker.js",
		"auto": "npm-run-all auto:*",
		"eslint": "eslint ./ --ext .ts,.tsx,.js,.jsx --fix",
		"eslint:test": "eslint ./lib/index.tsx --fix",
		"cz:init": "commitizen init cz-conventional-changelog --save --save-exact",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
	},
	"engines": {
		"npm": "6.10.1",
		"node": ">=10.13.0"
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"core-js": "^3.2.1",
		"height-zero2auto": "^1.0.8",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-transition-group": "^4.3.0"
	},
	"peerDependencies": {
		"prop-types": "^15.6.0",
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "7.4.3",
		"@types/classnames": "^2.2.9",
		"@types/jest": "^24.0.16",
		"@types/node": "^12.6.8",
		"@types/react": "^16.8.23",
		"@types/react-dom": "^16.8.5",
		"@types/react-intl": "^2.3.18",
		"@types/react-router-dom": "^4.3.4",
		"@typescript-eslint/eslint-plugin": "^2.3.3",
		"@typescript-eslint/parser": "^2.3.3",
		"babel-eslint": "^10.0.2",
		"babel-jest": "^24.8.0",
		"babel-loader": "8.0.5",
		"babel-plugin-import": "^1.12.0",
		"babel-plugin-named-asset-import": "^0.3.2",
		"babel-plugin-transform-rename-import": "^2.3.0",
		"babel-preset-react-app": "^9.0.0",
		"cheerio": "^1.0.0-rc.3",
		"clean-webpack-plugin": "^3.0.0",
		"commitizen": "^4.0.3",
		"compass-importer": "^0.4.1",
		"computed-style": "^0.3.0",
		"conventional-changelog-cli": "^2.0.23",
		"cross-env": "^5.2.0",
		"css-loader": "2.1.1",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.14.0",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-loader": "^3.0.2",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.14.3",
		"eslint-plugin-react-hooks": "^2.1.2",
		"fork-ts-checker-webpack-plugin": "^1.5.0",
		"husky": "^3.0.5",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.7.1",
		"jest-puppeteer": "^4.3.0",
		"lint-staged": "^9.2.5",
		"mini-css-extract-plugin": "0.5.0",
		"node-sass": "^4.12.0",
		"npm-run-all": "^4.1.5",
		"optimize-css-assets-webpack-plugin": "5.0.1",
		"prettier": "^1.18.2",
		"react-flexbox-grid": "^2.1.2",
		"react-intl": "^2.9.0",
		"react-spring": "^8.0.27",
		"sass-loader": "7.1.0",
		"svg-to-react": "^0.1.0",
		"svgo": "^1.3.0",
		"terser-webpack-plugin": "1.2.3",
		"thread-loader": "^2.1.3",
		"typescript": "^3.6.3",
		"webpack": "4.35.0",
		"webpack-chain": "^6.0.0",
		"webpack-dev-server": "3.2.1",
		"webpack-progress-bar": "^1.0.6"
	},
	"license": "Apache-2.0",
	"repository": "https://github.com/sewerganger/silent-concept",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		},
		"webgradients": "https://webgradients.com/",
		"libSvg": {
			"target": "./lib/assets/svg",
			"src": "./assets"
		}
	}
}
