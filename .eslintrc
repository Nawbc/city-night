{
	"env": {
		"es6": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"sourceType": "module",
		"experimentalObjectRestSpread": true,
		"jsx": true,
		"ecmaVersion": 8
	},
	"parser": "babel-eslint",
	"rules": {
		"no-console": "off",
		"no-inner-declarations": "off"
	}
}
