{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": [
						"last 2 versions",
						"ie >= 8"
					]
				},
				"loose": true,
				"useBuiltIns": "usage",
				"corejs": "2",
			}
		],
		[
			"@babel/preset-react"
		],
		[
			"@babel/preset-typescript",
			{
				"isTSX": true,
				"allExtensions": true,
			}
		]
	],
	"plugins": [
		[
			"@babel/plugin-proposal-class-properties",
			{
				"loose": true
			}
		],
		[
			"@babel/plugin-syntax-dynamic-import"
		],
		[
			"@babel/plugin-transform-runtime"
		],
	]
}
